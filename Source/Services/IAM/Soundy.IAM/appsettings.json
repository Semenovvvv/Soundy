{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "JwtConfig": {
    "Secret": "a_very_strong_secret_key_for_jwt_token_generation_should_be_at_least_256_bits",
    "Issuer": "soundy-iam",
    "Audience": "soundy-services",
    "AccessTokenExpirationMinutes": 30,
    "RefreshTokenExpirationDays": 7
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=Soundy-IAM;User Id=sa;Password=Your_password123;TrustServerCertificate=True;"
  }
}
